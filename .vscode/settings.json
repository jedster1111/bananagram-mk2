{
  "eslint.workingDirectories": [
    "./packages/bananagram-frontend",
    "./packages/bananagram-common"
  ],
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    {
      "language": "typescript",
      "autoFix": true
    },
    {
      "language": "typescriptreact",
      "autoFix": true
    }
  ],
  "cSpell.words": ["lineinfile", "testid"],
  "cSpell.diagnosticLevel": "Hint",
  "coverage-gutters.coverageReportFileName": "bananagram-frontend/coverage/**/index.html",
  "typescript.tsdk": "node_modules/typescript/lib",
  "jest.pathToJest": "yarn workspaces run test"
}
